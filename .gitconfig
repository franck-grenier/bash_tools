[alias]
  s = status -sb
  state='git fetch --prune ; git fetch --tags ; clear && git branch -vv && git status'
  d = diff
  co = checkout
  br = branch
  last = log -1 HEAD
  cane = commit --amend --no-edit
  lo = log --oneline -n 10
  pr = pull --rebase
  rh='git reset --hard'
  remotes='git remote -v'
  fetchall='git fetch --all'
